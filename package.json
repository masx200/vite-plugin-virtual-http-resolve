{
  "name": "@masx200/vite-plugin-virtual-http-resolve",
  "version": "1.0.1",
  "pravite": false,
  "description": "A Vite plugin that resolves remote HTTP/HTTPS modules to virtual modules with caching support",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "format": "prettier --write .",
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "plugin",
    "http",
    "https",
    "virtual",
    "remote",
    "module",
    "resolve",
    "cache"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "fs-extra": "^11.0.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "type": "module"
}
